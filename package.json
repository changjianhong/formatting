{
	"name": "formatting",
	"displayName": "formatting",
	"description": "format owner",
	"version": "0.0.1",
	"publisher": "changjianhong",
	"icon": "assets/launch.png",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:formatting.formatImport",
		"onCommand:formatting.formatStylus"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/changjianhong/formatting"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "formatting.formatImport",
				"title": "format import"
			},
			{
				"command": "formatting.formatStylus",
				"title": "format stylus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"gen": "vsce package",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
